@{
    ViewData["Title"] = "Project";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<section>

    <div class="d-flex justify-content-between gap-4">
        <article class="d-grid flex-grow-1">
            @* list item *@
            <div class="bg-white px-3 py-2 shadow rounded">
                <table class="table table-bordered">
                    <thead>
                        <tr class="text-center">
                            <th>STT</th>
                            <th>Project Name</th>                       
                            <th>Send User</th>
                            <th>Due Day</th>                            
                            <th>Sent time</th>                            
                            <th>AttachFile</th>                            
                            
                            <th>Accepted</th>                            
                            
                        </tr>
                    </thead>
                    <tbody id="listProjectReceived">
                        <tr>
                            <td colspan="8" class="text-center">No data</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </article>
    </div>
</section>

@section Scripts {
    <script src="~/js/cart.js"></script>
}